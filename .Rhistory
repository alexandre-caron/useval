ggplot() + aes(x = n, y = value, group = measure, color = measure) +
geom_line(size =1) +
scale_x_continuous(breaks = res_table$n) +
xlab("Sample size (n')") + ylab("thousand Euro (Kâ‚¬)") +
labs(color = "Value of Information measure: ") +
